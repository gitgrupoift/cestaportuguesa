<?xml version="1.0" encoding="utf-8"?>
<bundle name="WooSupply">
  <domain name="woosupply-lite">
    <project name="WooSupply" slug="woosupply">
      <source>
        <directory>.</directory>
				<exclude>
					<directory>assets</directory>
					<directory>modules</directory>
				</exclude>
      </source>
      <target>
        <directory>languages</directory>
      </target>
      <template>
        <file>languages/woosupply-lite.pot</file>
      </template>
    </project>
  </domain>
  <domain name="lws-adminpanel">
    <project name="LWS AdminPanel" slug="lws-adminpanel">
      <source>
        <directory>./assets/lws-adminpanel</directory>
      </source>
      <target>
        <directory>./assets/lws-adminpanel/languages</directory>
      </target>
      <template>
        <file>./assets/lws-adminpanel/languages/lws-adminpanel.pot</file>
      </template>
    </project>
  </domain>
  <domain name="woosupply-pro">
    <project name="WooSupply Pro" slug="woosupply-pro">
      <source>
        <directory>./modules/woosupply-pro</directory>
      </source>
      <target>
        <directory>./modules/woosupply-pro/languages</directory>
      </target>
      <template>
        <file>./modules/woosupply-pro/languages/woosupply-pro.pot</file>
      </template>
    </project>
  </domain>
  <exclude>
    <directory>./assets/tgm</directory>
  </exclude>
</bundle>
